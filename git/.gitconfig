[user]
  name = motomux
  email = motomux@gmail.com
[core]
  excludesfile = ~/.gitignore
  pager = less -r
  editor = vim
[color]
  ui = auto
[push]
  default = current
[branch]
  autosetuprebase = always
[merge]
  ff = false
[pull]
  rebase = true
[alias]
  a = add
  ai = add -i
  au = add -u
  b = branch
  ba = branch -a
  ci = commit
  co = checkout
  com = checkout master
  cod = checkout develop
  d = diff
  dc = diff --cached
  f = fetch --prune --tag
  l = log --graph -n 20 --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(green)- %an, %cr%Creset'
  ll = log --stat --abbrev-commit
  ln = log --graph -n 20 --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(green)- %an, %cr%Creset' --name-status
  lp = log --oneline -n 20 -p
  ls = log --stat --abbrev-commit -n 1
  rb = rebase
  rbi = rebase -i
  rbm = rebase master
  rbd = rebase develop 
  s = status --short --branch
  st = status
  sh = show
[github]
  user = motomux
